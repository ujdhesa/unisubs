msgid "avoid more than 2 lines per subtitle."
msgstr "shmangni më shumë se 2 rreshta për titull."

#: templates/subtitles/subtitle-editor.html:453
msgid "Minimum duration"
msgstr "Kohëzgjatje minimum"

#: templates/subtitles/subtitle-editor.html:454
msgid "Subtitles should not be less than 1 second."
msgstr "Titra s’duhet të jenë më të shkurtër se 1 sekondë."

#: templates/subtitles/subtitle-editor.html:457
msgid "Maximum duration"
msgstr "Kohëzgjatje maksimum"

#: templates/subtitles/subtitle-editor.html:458
msgid "Split subtitles lasting more than 7 seconds."
msgstr "Titra që zgjasin më shumë se 7 sekonda, ndajini."

#: templates/subtitles/subtitle-editor.html:461
msgid "Reading speed"
msgstr "Shpejtësi leximi"

#: templates/subtitles/subtitle-editor.html:462
msgid "8-25 characters/second is recommended;"
msgstr "Këshillohet 8-25 shenja/sekondë;"

#: templates/subtitles/subtitle-editor.html:463
msgid "8-16 characters/second for children."
msgstr "8-16 shenja/sekondë për fëmijë."

#: templates/subtitles/subtitle-editor.html:466
msgid "Sounds"
msgstr "Tinguj"

#: templates/subtitles/subtitle-editor.html:467
msgid "Describe meaningful sounds that are relevant to the plot."
