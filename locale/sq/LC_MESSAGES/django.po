#: templates/subtitles/subtitle-editor.html:443
msgid "Amara Style Guidelines"
msgstr "Udhëzime Stili te Amara"

#: templates/subtitles/subtitle-editor.html:448
msgid "Subtitle line length"
msgstr "Gjatësi rreshti titrash"

#: templates/subtitles/subtitle-editor.html:449
